{
    "Version": "2008-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Principal": {
          "AWS": "arn:aws:iam::cloudfront:user/CloudFront Origin Access Identity {{cloudfront-oai-id}}"
        },
        "Action": "s3:GetObject",
        "Resource": "arn:aws:s3:::{{s3-bucket-name}}/*"
      }
    ]
}